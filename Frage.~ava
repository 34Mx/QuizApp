public class Frage {
  private String frage = "";
  private String loesung = "";
  private String tipp = "";
  
  public Frage(String pFrage, String pLoesung, String pTipp) {
    frage = pFrage;
    loesung = pLoesung;
    tipp = pTipp;
  }
  
  public String getFrage() {
    return frage;
  }
  
  public String getLoesung() {
    return loesung;
  }
  
  public String getTipp() {
    return tipp;
  }
}